{
	"name": "Tokyo (Light)",
	"type": "light",
	"semanticHighlighting": true,
	"colors": {
		"editor.background": "#ffffff",
		"editor.foreground": "#24292e",
		"titleBar.activeBackground": "#ffffff",
		"titleBar.inactiveBackground": "#ffffff",
		"activityBarBadge.background": "#1abc9c",
		"activityBar.background": "#ffffff",
		"activityBar.activeBorder": "#1abc9c",
		"activityBar.foreground": "#12977d",
		"activityBar.inactiveForeground": "#24292e",
		"list.activeSelectionForeground": "#24292e",
		"list.activeSelectionBackground": "#1abc9c",
		"sideBar.border": "#ffffff",
		"sideBar.background": "#ffffff",
		"sideBarTitle.foreground": "#bbbbbb",
		"sideBarSectionHeader.foreground": "#24292e",
		"sideBarSectionHeader.background": "#ffffff",
		"statusBar.background": "#ffffff",
		"statusBar.foreground": "#24292e",
		"editorWidget.background": "#ffffff",
		"editorGroupHeader.tabsBackground": "#ffffff",
		"editorGroupHeader.tabsBorder": "#ffffff",
		"tab.activeBackground": "#ffffff",
		"tab.inactiveBackground": "#ffffff",
		"tab.activeBorderTop": "#1abc9c",
		"tab.activeForeground": "#24292e",
		"breadcrumb.background": "#ffffff",
		"tab.border": "#6e6e6e00",
		"terminal.ansiBlue": "#009c8f",
		"terminal.ansiBlack": "#232627",
		"terminal.ansiBrightBlack": "#464e4f",
		"terminal.ansiBrightBlue": "#00b6a4",
		"terminal.ansiBrightCyan": "#16a085",
		"terminal.ansiCyan": "#17b898",
		"terminal.ansiBrightGreen": "#7f990d",
		"terminal.ansiBrightMagenta": "#8e44ad",
		"terminal.ansiBrightRed": "#ca1111",
		"terminal.ansiBrightWhite": "#5d5c5a",
		"terminal.ansiBrightYellow": "#9e6200",
		"terminal.ansiGreen": "#738d01",
		"terminal.ansiMagenta": "#aa62c8",
		"terminal.ansiRed": "#BF0C0C",
		"terminal.ansiWhite": "#42413f",
		"terminal.ansiYellow": "#d59200",
		"terminal.foreground": "#24292e",
		"terminal.background": "#ffffff",
		"editor.selectionBackground": "#9ce0d3",
		"input.foreground": "#24292e",
		"input.background": "#ffffff",
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#657B83"
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded",
				"string meta.image.inline.markdown",
				"variable.legacy.builtin.python"
			],
			"settings": {
				"foreground": "#009f9f"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#93A1A1"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#657B83"
			}
		},
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#657B83"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ca1111"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable.language",
				"variable.other"
			],
			"settings": {
				"foreground": "#7f990d"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ca1111"
			}
		},
		{
			"name": "Keyword -- Control",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#16a085"
			}
		},
		{
			"name": "Keyword -- Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#8f008f"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#8f008f"
			}
		},
		{
			"name": "Class name",
			"scope": [
				"entity.name.class",
				"entity.name.type",
				"entity.name.namespace",
				"entity.name.scope-resolution"
			],
			"settings": {
				"foreground": "#7f990d"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#7f990d"
			}
		},
		{
			"name": "Variable start",
			"scope": "punctuation.definition.variable",
			"settings": {
				"foreground": "#8E44AD"
			}
		},
		{
			"name": "Embedded code markers",
			"scope": [
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end"
			],
			"settings": {
				"foreground": "#DC322F"
			}
		},
		{
			"name": "Built-in constant",
			"scope": [
				"constant.language",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#8E44AD"
			}
		},
		{
			"name": "Support.construct",
			"scope": [
				"support.function.construct",
				"keyword.other.new"
			],
			"settings": {
				"foreground": "#8E44AD"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#ca1111"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#9db502"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#7f990d"
			}
		},
		{
			"name": "Tag start/end",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#93A1A1"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#93A1A1"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"foreground": "#7f990d"
			}
		},
		{
			"name": "Continuation",
			"scope": "punctuation.separator.continuation", 
			"settings": {
				"foreground": "#DC322F"
			}
		},
		{
			"name": "Library constant",
			"scope": [
				"support.constant",
				"support.variable"
			],
			"settings": {}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#7f990d"
			}
		},
		{
			"name": "Library Exception",
			"scope": "support.type.exception",
			"settings": {
				"foreground": "#8E44AD"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#DC322F"
			}
		},
		{
			"name": "diff: header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7f990d"
			}
		},
		{
			"name": "diff: deleted",
			"scope": "markup.deleted",
			"settings": {
				"fontStyle": "",
				"foreground": "#DC322F"
			}
		},
		{
			"name": "diff: changed",
			"scope": "markup.changed",
			"settings": {
				"fontStyle": "",
				"foreground": "#8E44AD"
			}
		},
		{
			"name": "diff: inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#ca1111"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#ca1111"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#ca1111"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#D33682"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#2AA198"
			}
		},
		{
			"name": "Markup Headings",
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7f990d"
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading.setext",
			"settings": {
				"fontStyle": "",
				"foreground": "#7f990d"
			}
		}
	]
}